{
  "name": "creative-boilerplate",
  "version": "1.0.0",
  "author": {
    "name": "Cl√°udio Mindouro",
    "email": "claudiopedrom@gmail.com",
    "url": "https://www.mindouro.com/"
  },
  "description": "A boilerplate for creative code",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claudiopedrom/creative-boilerplate"
  },
  "keywords": [
    "creative",
    "boilerplate",
    "webgl",
    "threejs",
    "gsap",
    "webpack",
    "express",
    "pug"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev:backend": "NODE_ENV=development nodemon server/index.js",
    "build:backend": "NODE_ENV=production node server/index.js",
    "dev:frontend": "webpack serve --progress --config webpack.config.development.js",
    "build:frontend": "webpack --progress --config webpack.config.build.js",
    "dev": "npm-run-all --parallel dev:*",
    "lint": "npm-run-all --parallel lint:*",
    "lint:scripts": "eslint 'src/**/*.js' --max-warnings=0 --fix",
    "lint:styles": "stylelint 'src/**/*.scss' --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint:scripts"
    ],
    "src/**/*.scss": [
      "npm run lint:styles"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "autoprefixer": "10.3.1",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "6.2.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "glslify-loader": "2.0.0",
    "husky": "7.0.1",
    "image-minimizer-webpack-plugin": "2.2.0",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "9.0.0",
    "lint-staged": "11.1.2",
    "mini-css-extract-plugin": "2.2.0",
    "nodemon": "2.0.12",
    "npm-run-all": "4.1.5",
    "postcss-loader": "6.1.1",
    "prettier": "2.3.2",
    "sass": "1.37.5",
    "sass-loader": "12.1.0",
    "style-loader": "3.2.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recess-order": "2.4.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-prettier": "1.2.0",
    "webpack": "5.48.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "7.15.3",
    "@prismicio/client": "5.1.0",
    "dotenv": "10.0.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "gsap": "3.7.1",
    "include-media": "1.4.10",
    "lodash": "4.17.21",
    "prismic-dom": "2.2.6",
    "pug": "3.0.2"
  }
}
